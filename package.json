    "@typescript-eslint/parser": "^8.32.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jwt-decode": "^4.0.0",
