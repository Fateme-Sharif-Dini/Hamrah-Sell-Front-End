    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "commitizen": "^4.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.17.0",
