{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-check": "next lint --fix",
    "type-check": "npx tsc",
    "format": "prettier --write .",
    "commit": "git add . && cz",
    "prepare": "husky",
    "clean-icons": "rimraf src/components",
    "generate-icons": "pnpm clean-icons && svgr --icon  --typescript --filename-case kebab --svg-props width=24,height=24 --replace-attr-values \"#000=currentColor,#fff=currentColor\" --out-dir src/app/components/icons svgs"
  },
