    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@typescript-eslint/parser": "^8.32.0",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.484.0",
    "next": "15.2.4",
    "next-auth": "5.0.0-beta.27",
    "next-themes": "^0.4.6",
