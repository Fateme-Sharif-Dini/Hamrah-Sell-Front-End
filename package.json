    "@tailwindcss/postcss": "^4.1.5",
    "@types/node": "^22.15.16",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/tailwindcss": "^3.1.0",
    "commitizen": "^4.3.1",
