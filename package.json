{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-check": "next lint --fix",
    "type-check": "npx tsc",
    "format": "prettier --write .",
    "commit": "git add . && cz",
